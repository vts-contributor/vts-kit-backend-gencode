[{
  "className": "Employees",
  "desc": "Lớp thao tác danh sach phòng",
  "listMethod": [
    {
      "type": "get",
      "name": "findEmployeebyId2",
      "value": "/v1/employee2/{id}",
      "sql": "SELECT * FROM HR_EMPLOYEES WHERE ID = :id",
      "params":["id"],
      "desc": "Lay thong tin nhan vien theo id",
      "jpa": true
    },
    {
      "type": "delete",
      "name": "deleteEmployee",
      "value": "/v1/delete/{id}",
      "sql": "DELETE FROM HR_EMPLOYEES WHERE ID = :id ",
      "params":[],
      "desc": "xoa nhan vien theo id",
      "jpa": true
    },
    {
      "type": "put",
      "name": "updateEmployee",
      "value": "/v1/employee/{id}",
      "sql": "update HR_EMPLOYEES set EMPLOYEE_ID = :employeeId, FULL_NAME = :fullName WHERE ID = :id",
      "params":["id", "employeeId", "fullName"],
      "desc": "Cap nhat thong tin",
      "jpa": false
    }
  ]
},

  {
    "className": "Rooms",
    "desc": "Lớp thao tác danh sach phong",
    "listMethod": [
      {
